name: Dependents Events

on: push

jobs:
  show-info:
    runs-on: ubuntu-latest
    steps:
      - name: all events info
        run: |
          echo "This is a push event on all the branches"

  test:
    runs-on: ubuntu-latest
    needs: show-info
    steps:
      - name: Checkout code
        uses: actions/checkout@v4
