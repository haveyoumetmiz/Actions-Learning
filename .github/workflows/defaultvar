name: Default Variable 

on:
  push:
    branches: [main]

jobs:
  Default-Variable:
    runs-on: ubuntu-latest
    steps:
      - name: Display Default Variable
        run: |
          echo 'Repository: $GITHUB_REPOSITORY'
          echo 'Action: $GITHUB_ACTIOR"
          echo 'SHA: $GITHUB_SHA'
          echo 'Event Name: $GITHUB_EVENT_NAME'
